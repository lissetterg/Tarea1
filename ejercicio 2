#include <stdio.h>
int main ()
{
int a, b;
b=0;

while (b <= 10){

printf("Ingrese un valor \n");

scanf("%d", &a);

if (a<10 || a>100) {

if(a>100){ 
printf ("su numero es mayor que 100 \n");
}else{
printf ("su numero es menor que 10 \n");
}

} else {
printf ("su numero se encuentra entre 10 y 100 \n");
}


b = b+1;
}

return 0;
}
